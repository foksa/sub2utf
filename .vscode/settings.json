{
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment.block.percentage.markdown",
        "settings": {
          "foreground": "#FFA500",
          "fontStyle": "italic"
        }
      }
    ]
  },
  "todohighlight.include": [
    "**/*.md",
    "**/*.markdown"
  ],
  "todohighlight.keywords": [
    {
      "text": "%%",
      "color": "rgb(150, 135, 60)",
      "backgroundColor": "transparent"
    },
    {
      "text": "•%%>",
      "color": "rgb(60, 140, 140)",
      "backgroundColor": "transparent"
    },
    {
      "text": "<%%•",
      "color": "rgb(60, 140, 140)",
      "backgroundColor": "transparent"
    }
  ]
}